{
  "presets": [
    "env",
    "react",
    "stage-1"
  ],
  "plugins": [
    [
      "transform-decorators-legacy",
      "transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    },
      "transform-function-bind"
    ]
  ]
}